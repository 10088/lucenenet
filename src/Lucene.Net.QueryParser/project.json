{
  "version": "4.8.0-alpha",
  "dependencies": {
    "Lucene.Net.Analysis.Common": "4.8.0-alpha",
    "Lucene.Net.Queries": "4.8.0-alpha",
    "Lucene.Net.Sandbox": "4.8.0-alpha"
  },
  "frameworks": {
    "netstandard1.5": {
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Xml.XmlDocument": "4.0.1"
      },
      "buildOptions": {
        "compile": {
          "excludeFiles": [
            "Xml/QueryTemplateManager.cs"
          ]
        }
      }
    },
    "net451": {
      "buildOptions": {
        "define": [ "FEATURE_SERIALIZABLE", "FEATURE_CLONEABLE" ]
      },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    }
  }
}
