    "owners": [
      "The Apache Software Foundation"
    ],
    "repository": {
      "url": "https://github.com/apache/lucenenet"
    },
    "tags": [ "lucene.net", "core", "text", "search", "information", "retrieval", "lucene", "apache", "analysis", "index", "query" ]
  },
  "frameworks": {
    "netstandard1.5": {
      "imports": [ "dnxcore50", "portable-net403+sl5+win8+wp8+wpa81" ],
      "buildOptions": {
        "debugType": "portable",
        "define": [ "NETSTANDARD" ],
        "compile": {
          "includeFiles": [
            "../CommonAssemblyInfo.cs"
          ]
        }
      },
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    },
    "net451": {
      "dependencies": {
        "Lucene.Net": "4.8.0"
      },
      "buildOptions": {
        "debugType": "portable",
        "define": [ "FEATURE_SERIALIZABLE" ],
        "compile": {
          "includeFiles": [
            "../CommonAssemblyInfo.cs"
          ]
        }
      }
    }
  }
}