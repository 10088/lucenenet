{
  "version": "4.8.0",
  "dependencies": {
    "dotnet-test-nunit": "3.4.0-beta-3",
    "Lucene.Net.Highlighter": "4.8.0",
    "Lucene.Net.Queries": "4.8.0",
    "Lucene.Net.TestFramework": "4.8.0",
    "NUnit": "3.5.0"
  },
  "testRunner": "nunit",
  "frameworks": {
    "netcoreapp1.0": {
      "buildOptions": {
        "debugType": "portable",
        "define": [ "NETSTANDARD" ],
        "compile": {
          "includeFiles": [
            "../CommonAssemblyInfo.cs"
          ],
          "excludeFiles": [
            "Support/TestApiConsistency.cs"
          ]
        },
        "embed": {
          "includeFiles": [
            "PostingsHighlight/CambridgeMA.utf8"
          ]
        }
      },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.1"
        },
        "System.Xml.XmlDocument": "4.0.1"
      },
      "imports": "dnxcore50"
    },
    "net451": {
      "buildOptions": {
        "debugType": "portable",
        "define": [ "FEATURE_BREAKITERATOR", "FEATURE_SERIALIZABLE" ],
        "compile": {
          "includeFiles": [
            "../CommonAssemblyInfo.cs"
          ]
        },
        "embed": {
          "includeFiles": [
            "PostingsHighlight/CambridgeMA.utf8"
          ]
        }
      },
      "dependencies": {
        "icu.net": "2.1.1-beta0003",
        "Icu4c.Win.Full.Lib": "56.1.21"
      }
    }
  },

  "runtimes": {
    "win7-x86": {},
    "win7-x64": {}
  }
}
