{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Lucene.Net/Lucene.Net.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "src": "../../src"
        }
      ],
      "dest": "obj/docfx/api/core",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "overview.md"
        ],
        "src": "../../src/Lucene.Net",
        "dest": "docs"
      },
      {
        "files": [
          "**.yml",
          "**.md"
        ],
        "src": "obj/docfx/api/core",
        "dest": "docs"
      },
      {
        "files": [
          "toc/core/toc.yml"
        ],
        "dest": "docs"
      }
    ],
    "resource": [
      {
        "files": [
          "logo/favicon.ico",
          "logo/lucene-net-icon-64x64.png",
          "logo/lucene-net-color.png",
          "logo/lucene-net-reverse-color.png"
        ],
        "src": "../../branding"
      }
    ],
    "overwrite": [     
      {
        "files": [
          "apiSpec/core/**/*.md"
        ]
      }
    ],
    "dest": "_site/api/core",
    "globalMetadataFiles": [
      "docfx.global.json"
    ],
    "template": [
      "default",
      "lucenetemplate"
    ],
    "postProcessors": [],
    "markdownEngineName": "dfm",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}